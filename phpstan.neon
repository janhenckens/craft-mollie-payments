includes:
    - vendor/craftcms/phpstan/phpstan.neon

parameters:
    level: 1
    paths:
        - src
    excludePaths:
        analyse:
            - modules/*/node_modules/*
            - modules/*/vendor/*
            - modules/*/tests/*
        analyseAndScan:
            - modules/*/node_modules/*
            - modules/*/vendor/*
            - modules/*/tests/*